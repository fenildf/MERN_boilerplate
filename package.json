{
  "name": "MERN_boilerplate",
  "version": "1.0.0",
  "private": "true",
  "description": "Boilerplate for Mongo Express React Node",
  "main": "index.js",
  "scripts": {
    "start": "node webapp.js",
    "watch": "babel --presets react src --watch --out-dir static",
